{
  "name": "metalsmith-framework",
  "private": true,
  "devDependencies": {
    "forever": "0.15.3",
    "chokidar": "1.7.0",
    "pug": "2.0.0-rc.4",
    "metalsmith": "2.3.0",
    "metalsmith-watch": "1.0.3",
    "metalsmith-layouts": "1.8.1",
    "metalsmith-markdown": "0.2.1",
    "metalsmith-permalinks": "0.5.0",
    "metalsmith-static": "0.0.5",
    "metalsmith-fingerprint": "1.0.3",
    "lodash.get": "4.4.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0",
    "extract-text-webpack-plugin": "git://github.com/webpack-contrib/extract-text-webpack-plugin.git#v2.0.0-rc.2",
    "webpack-manifest-plugin": "1.1.0",
    "raw-loader": "0.5.1",
    "file-loader": "0.10.0",
    "url-loader": "0.5.7",
    "style-loader": "0.13.1",
    "css-loader": "0.26.1",
    "postcss-loader": "1.2.2",
    "postcss-import": "9.1.0",
    "postcss-assets": "4.1.0",
    "postcss-color-mix": "1.1.0",
    "postcss-color-function": "3.0.0",
    "postcss-responsive-images": "1.0.3",
    "postcss-media-minmax": "2.1.2",
    "postcss-cachebuster": "0.1.4",
    "lost": "8.0.0",
    "precss": "1.4.0",
    "autoprefixer": "6.7.2",
    "postcss-responsive-type": "0.5.0",
    "postcss-font-magician": "1.6.0",
    "normalize.css": "5.0.0"
  },
  "dependencies": {
    "express": "4.14.1",
    "morgan": "1.8.1"
  },
  "scripts": {
    "build": "NODE_ENV=production node build",
    "dev": "node_modules/.bin/forever --minUptime 1000 --spinSleepTime 1000 node_modules/.bin/webpack-dev-server --colors --content-base built/ --host 0.0.0.0 --port 80 --hot --inline",
    "watch": "node_modules/.bin/forever --minUptime 1000 --spinSleepTime 1000 build.js",
    "start": "env $([ ! -f ../ENVIRONMENT_LOCAL ] || (cat ../ENVIRONMENT_LOCAL | xargs)) node_modules/.bin/forever --minUptime 1000 --spinSleepTime 1000 index.js",
    "production": "NODE_ENV=production node index",
    "webpack": "node_modules/.bin/webpack"
  }
}
